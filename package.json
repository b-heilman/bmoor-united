{
  "name": "bmoor-united",
  "version": "0.1.0",
  "workspaces": [
    "packages/*"
  ],
  "engines": {
    "node": ">=16",
    "npm": ">=8"
  },
  "private": true,
  "author": "Brian Heilman <das.ist.junk@gmail.com>",
  "description": "A collection of data management and engineering libraries",
  "license": "MIT",
  "homepage": "https://github.com/b-heilman/bmoor-united#readme",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/b-heilman/bmoor-united.git"
  },
  "bugs": {
    "url": "https://github.com/b-heilman/bmoor-united/issues"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "chai": "^4.3.6",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jshint": "^2.13.4",
    "jshint-stylish": "2.1.0",
    "mocha": "^9.2.2",
    "prettier": "2.4.1",
    "sinon": "^7.5.0",
    "typescript": "^4.6.3",
    "yargs": "^15.3.1"
  },
  "scripts": {
    "lint": "node ./node_modules/eslint/bin/eslint ./packages/*/src",
    "test": "mocha --recursive \"./packages/*/src/**/*.spec.js\"",
    "test:file": "mocha",
    "prettier": "npx prettier --write ./packages/*/src",
    "finalize": "npm run prettier && npm run lint &&  npm run test"
  }
}
